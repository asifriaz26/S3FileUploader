{
    "name": "sudocoder/s3fileuploader",
    "description": "This package makes it easy to handle files on Amazon S3 for multiple systems. You can upload, copy between buckets, check if a file exists, and download files â€“ all by just providing bucket details and file names. Simple file management for smooth data handling.",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "aws/aws-sdk-php": "^3.300"
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php ../artisan package:discover --ansi"
        ]
    },
    "autoload": {
        "files": ["src/Facades/S3FileUpload.php"],
        "psr-4": {
            "SudoCoder\\S3FileUploader\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SudoCoder\\S3FileUploader\\Providers\\S3UploadFileServiceProvider"
            ],
            "aliases": {
                "S3FileUpload": "SudoCoder\\S3FileUploader\\Facades\\s3FileUpload"
            }
        }
    }
}
